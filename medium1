# Practice Problems: Medium 1 in PY101

# question 1

for round in range(1,11):
    tmp = '-' * round
    print(F"{tmp}The Flintstones Rock!")

# question 2
def factors(number):
    divisor = number
    result = []
    while divisor > 0:
        if number % divisor == 0:
            result.append(number // divisor)
        divisor -= 1
    return result

 
